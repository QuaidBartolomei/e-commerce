ory,\n    id,\n    imageUrls,\n    name,\n    price,\n    inventory,\n  };\n  return baseItem;\n}\n\nexport async function generateItemAndAddToDb() {\n  let item = generateShopItem();\n  await addShopItem(item);\n  return console.log(item);\n}\n",["243","244"],"/home/quaid/portfolio-projects/e-commerce/src/utils/list.utils.ts",[],"/home/quaid/portfolio-projects/e-commerce/src/utils/number.utils.ts",[],"/home/quaid/portfolio-projects/e-commerce/src/pages/ItemDetails/useColorSelect.tsx",[],{"ruleId":"245","replacedBy":"246"},{"ruleId":"247","replacedBy":"248"},{"ruleId":"249","severity":1,"message":"250","line":9,"column":8,"nodeType":"251","messageId":"252","endLine":9,"endColumn":13},{"ruleId":"253","severity":1,"message":"254","line":40,"column":6,"nodeType":"255","endLine":40,"endColumn":8,"suggestions":"256"},{"ruleId":"249","severity":1,"message":"257","line":9,"column":24,"nodeType":"251","messageId":"252","endLine":9,"endColumn":31},{"ruleId":"249","severity":1,"message":"258","line":23,"column":37,"nodeType":"251","messageId":"252","endLine":23,"endColumn":52},{"ruleId":"249","severity":1,"message":"259","line":4,"column":3,"nodeType":"251","messageId":"252","endLine":4,"endColumn":15},{"ruleId":"245","replacedBy":"260"},{"ruleId":"247","replacedBy":"261"},"no-native-reassign",["262"],"no-negated-in-lhs",["263"],"@typescript-eslint/no-unused-vars","'types' is defined but never used.","Identifier","unusedVar","react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'state.isAuth'. Either include it or remove the dependency array.","ArrayExpression",["264"],"'Product' is defined but never used.","'defaultSelected' is assigned a value but never used.","'ClothingSize' is defined but never used.",["262"],["263"],"no-global-assign","no-unsafe-negation",{"desc":"265","fix":"266"},"Update the dependencies array to be: [state.isAuth]",{"range":"267","text":"268"},[1282,1284],"[state.isAuth]"]